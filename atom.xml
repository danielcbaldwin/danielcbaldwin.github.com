---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Daniel Baldwin</title>
 <link href="http://danielcbaldwin.com/atom.xml" rel="self"/>
 <link href="http://danielcbaldwin.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://danielcbaldwin.com/</id>
 <author>
   <name>Daniel Baldwin</name>
   <email>daniel@thebaldw.in</email>
 </author>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://danielcbaldwin.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://danielcbaldwin.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>